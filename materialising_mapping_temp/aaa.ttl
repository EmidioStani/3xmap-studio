@prefix ns1: <http://semweb.mmlab.be/ns/rml#> .
@prefix ns2: <http://www.w3.org/ns/r2rml#> .
@prefix ns3: <http://semweb.mmlab.be/ns/ql#> .
@prefix omap: <http://rdfolio.org/mapping/objectmap/> .
@prefix pomap: <http://rdfolio.org/mapping/predicateobjectmap/> .
@prefix smap: <http://rdfolio.org/mapping/subjectmap/> .
@prefix tmap: <http://rdfolio.org/mapping/triplesmap/> .

tmap:tm2 a ns2:TriplesMap ;
    ns1:logicalSource [ ns3:referenceFormulation ns3:CSV ;
            ns1:source "Authors - copia.csv" ] ;
    ns2:predicateObjectMap pomap:pom2 ;
    ns2:subjectMap smap:sm2 .

omap:om2 a ns2:ObjectMap ;
    ns2:constant "wefwe" ;
    ns2:termType ns2:Literal .

pomap:pom2 ns2:objectMap omap:om2 ;
    ns2:predicate tmap:efw .

smap:sm2 a ns2:SubjectMap ;
    ns2:constant tmap:wefwe ;
    ns2:termType ns2:IRI .

