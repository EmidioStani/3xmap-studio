@prefix ns1: <http://www.w3.org/ns/r2rml#> .
@prefix ns2: <http://semweb.mmlab.be/ns/rml#> .
@prefix ns3: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfolio: <http://rdfolio.org/mapping/> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdfolio:CoursesTriplesMap a ns1:TriplesMap ;
    ns2:logicalSource [ ns3:referenceFormulation ns3:CSV ;
            ns2:source "Physics_courses.csv" ] ;
    ns1:predicateObjectMap [ a ns1:PredicateObjectMap ;
            ns1:objectMap [ a ns1:ObjectMap ;
                    ns2:reference "course_name" ;
                    ns1:datatype xsd:string ;
                    ns1:termType ns1:Literal ] ;
            ns1:predicate schema:name ] ;
    ns1:subjectMap [ a ns1:SubjectMap ;
            ns1:class schema:Course ;
            ns1:template "http://example.org/Course/{course_id}" ;
            ns1:termType ns1:IRI ] .

