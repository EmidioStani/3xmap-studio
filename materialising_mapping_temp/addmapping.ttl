@prefix lt: <http://rdfolio.org/mapping/logicaltable/> .
@prefix omap: <http://rdfolio.org/mapping/objectmap/> .
@prefix pomap: <http://rdfolio.org/mapping/predicateobjectmap/> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix smap: <http://rdfolio.org/mapping/subjectmap/> .
@prefix tmap: <http://rdfolio.org/mapping/triplesmap/> .

tmap:tm1 a rr:TriplesMap ;
    rml:logicalSource [ ql:referenceFormulation ql:CSV ;
            rml:source "Authors.csv" ] ;
    rr:predicateObjectMap pomap:pom1 ;
    rr:subjectMap smap:sm1 .

omap:om1 a rr:ObjectMap ;
    rr:constant "fw" ;
    rr:termType rr:Literal .

pomap:pom1 rr:objectMap omap:om1 ;
    rr:predicate lt:fssds .

smap:sm1 a rr:SubjectMap ;
    rr:constant smap:sddfsf ;
    rr:termType rr:IRI .

